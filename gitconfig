	name = otaaaa
[core]
	excludesfile = ~/.gitignore_global
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
  sl = log --no-merges --date=short --pretty='format:%C(yellow)%h %C(green)%cd %C(blue)%an%C(red)%d %C(reset)%s'
  graph = log --graph -10 --branches --remotes --tags  --format=format:'%Cgreen%h %Cresetâ€¢ %<(75,trunc)%s (%cN, %ar) %Cred%d' --date-order
  prestage = diff -w
  branches = branch -a
  stashes = stash list
[commit]
	template = ~/.gitmessage

